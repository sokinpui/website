server {
    server_name xrayxrtas.space;

    location / {
        proxy_pass http://localhost:12352;
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header X-Forwarded-Proto $scheme;
    }

    access_log /var/log/nginx/your-website.com.access.log;
    error_log /var/log/nginx/your-website.com.error.log;

    listen 443 ssl; # managed by Certbot
        ssl_certificate /etc/letsencrypt/live/xrayxrtas.space/fullchain.pem; # managed by Certbot
        ssl_certificate_key /etc/letsencrypt/live/xrayxrtas.space/privkey.pem; # managed by Certbot
        include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
        ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}

server {
    if ($host = xrayxrtas.space) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    listen 80;
    server_name xrayxrtas.space;
    return 404; # managed by Certbot

}
